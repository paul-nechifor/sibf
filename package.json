{
  "name": "sibf",
  "version": "0.1.0",
  "author": "Paul Nechifor <paul@nechifor.net> (http://nechifor.net/)",
  "scripts": {
    "build": "intercessor",
    "clean": "rm -fr build html node_modules",
    "intercessor-make": "mkdir html 2>/dev/null; ./transform/transform.py",
    "start": "node build/app.js",
    "up": "npm install && npm run intercessor-make && npm run build && npm run start"
